  "scripts": {
    "clean:types": "rimraf ./dist/es/types",
    "clean:dist": "rimraf -g ./dist/*",
    "rollup": "npm run clean:dist && rollup -c --bundleConfigAsCjs && npm run copy && npm run clean:types",
    "cp4test": "node ./scripts/copyPackageForTest.cjs",
    "pub": "npm publish ./dist --scope=thuong98 --access public",
    "sync:lexical": "node ./scripts/sync-lexical-deps.cjs",
    "dev": "vite --host --port 3000",
    "build-dev": "vite build",
    "copy": "node ./scripts/packageJsonCopy.cjs",
    "build-prod": "vite build --mode production && npm run copy & npm run cp4test",
    "build-vercel": "(cd ../../ && node ./scripts/build.js) && npm run build-dev",
    "preview": "vite preview"
  },
